<div class="videoDesc__channel">
  {{#if channel.Thumbnail}}
  <img src="{{channel.Thumbnail}}&w=56&h=56" class="pfp" width="56" height="56" loading="lazy" />
  {{else}}
  <img src="/static/img/spaceman.png" class="pfp pfp--default" width="56" height="56" loading="lazy" />
  {{/if}}
  <p>
    {{#if channel.Title}}
    <b>{{channel.Title}}</b>
    <br>
    {{channel.Name}}
    {{else}}
    <b>{{channel.Name}}</b>
    {{/if}}
  </p>
</div>